                        // helpData += '<th>' + value.hospitalBeds + '</th>';
                        // helpData += '</tr>';

                        // console.log(value.state);
                    });
                    $('#collegeTable').append(helpData);
                });
            };

            var url = "https://api.rootnet.in/covid19-in/hospitals/beds";
            $.getJSON(url, function(data) {
                var helpData = '';
                $.each(data.data.regional, (key, value) => {
                    helpData += '<tr>';
                    helpData += '<th>' + value.state + '</th>';
                    helpData += '<th>' + value.ruralHospitals + '</th>';
                    helpData += '<th>' + value.ruralBeds + '</th>';
                    helpData += '<th>' + value.urbanHospitals + '</th>';
                    helpData += '<th>' + value.urbanBeds + '</th>';
                    helpData += '<th>' + value.totalHospitals + '</th>';
                    helpData += '<th>' + value.totalBeds + '</th>';
                    helpData += '</tr>';

                });
                $('#hospitalTable').append(helpData);
            });





        });
    </script>
