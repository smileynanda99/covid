    <script>
        $(document).ready(function() {


            function getList() {
                var url = "https://api.rootnet.in/covid19-in/hospitals/medical-colleges";
                $.getJSON(url, function(data) {
                    var helpData = '';
                    var state = $("#states").value;
                    var type = $("#type").value;
                    console.log(state, type);
                    $.each(data.data.medicalColleges, (key, value) => {
                        // if(value.state)
                        // helpData += '<tr>';
